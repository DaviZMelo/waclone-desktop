    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "npx yarn run build:electron && concurrently \"set BROWSER=none && react-scripts start\" \"wait-on http://localhost:3000 && electron core/electron/index.js\"",
    "build:react": "react-scripts build",
    "build:server": "tsc --project ./core/api/tsconfig.prod.json && tscpaths -p ./core/api/tsconfig.prod.json -s ./core/api/src -o ./core/api/dist",
    "build:electron": "yarn build:server && tsc core/electron/index.ts --OutDir core/electron",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build:electron && concurrently \"yarn build:react\"",
    "electron-pack": "electron-builder build -w "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
