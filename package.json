{
  "name": "waclone_desktop",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "main": "core/electron/index.js",
  "author": {
    "name": "WaClone",
    "email": "zacarias0022@outlook.com"
  },
  "build": {
    "extends": null,
    "asar": false,
    "appId": "com.davi-melo.wa-clone",
    "productName": "WaClone",
    "copyright": "Copyright Â© 2021 DaviZMelo",
    "files": [
      "package.json",
      "build/**/*",
      "node_modules/",
      "node_modules/**/*",
      "core/electron/index.js",
      "core/api/dist",
      "core/api/dist/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "core/electron/assets/icon.ico"
    },
    "nsis": {
      "installerIcon": "core/electron/assets/icon.ico",
      "installerHeaderIcon": "core/electron/assets/icon.ico",
      "deleteAppDataOnUninstall": true,
      "runAfterFinish": true,
      "createStartMenuShortcut": true,
      "createDesktopShortcut": true
    }
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.10",
